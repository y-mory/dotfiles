// keybindings.json
// 既定値を上書きするには、このファイル内にキー バインドを挿入しますauto[]
[
    {
        "key": "cmd+;",
        "command": "insertDateString.insertDate"
    },
    {
        "key": "shift+cmd+;",
        "command": "insertDateString.insertTime"
    },
    {
        "key": "cmd+`",
        "command": "workbench.action.terminal.new",
        "when": "terminalProcessSupported"
    }
]

